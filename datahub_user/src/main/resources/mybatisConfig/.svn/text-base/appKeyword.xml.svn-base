<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- LLBTMP_ADVERTISEMENT:广告表 -->
<mapper namespace="appKeyword">
  <resultMap id="appKeywordMap" type="_AppKeyword">
    <id property="id" column="id" />
    <result property="keywords" column="start_time"  />
    <collection property="apps" column="APP_ID" select="getBranch" ></collection>
	</resultMap>
	
	
	
	<select id="getBranch" resultType="_Branch">
		select * from llbtmp_branch where id = #{branch}
	</select>
	

	
	<select id="getCurrval" resultType="long">
	  select LLBTMP_AD_SEQ.currval from dual
	</select>
	
	


</mapper>